{
    "compilerOptions": {
      "module": "commonjs",
      "target": "es6",
      "outDir": "dist",
      "rootDir": ".",
      "sourceMap": true,
      "strict": true,
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "types": [
        "node",
      ],
      "lib": [
        "es2021"
      ],
      "plugins": [
        {
          "transform": "ts-auto-mock/transformer",
          "cacheBetweenTests": false
        }
      ]
    }
  }
  